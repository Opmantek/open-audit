<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>Installed - Visio</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[select oa_org.org_name, system.hostname, system.system_id, sys_sw_windows.windows_user_name, sys_sw_software.software_name, sys_sw_software.software_installed_by, sys_sw_software.software_version, sys_sw_software.software_installed_on from system left join oa_org on (system.man_org_id = oa_org.org_id) left join sys_sw_software on (system.system_id = sys_sw_software.system_id AND system.timestamp = sys_sw_software.timestamp) left join sys_sw_windows ON (system.system_id = sys_sw_windows.system_id AND system.timestamp = sys_sw_windows.timestamp) left join oa_group_sys on (system.system_id = oa_group_sys.system_id) WHERE (sys_sw_software.software_name = 'Microsoft Office Visio 2010' OR sys_sw_software.software_name LIKE 'Microsoft Office Visio Standard %' OR sys_sw_software.software_name LIKE 'Microsoft Office Visio Professional %' OR sys_sw_software.software_name LIKE 'Microsoft Office Visio Premium %' OR sys_sw_software.software_name LIKE 'Microsoft Visio Standard %' OR sys_sw_software.software_name LIKE 'Microsoft Visio Professional %' OR sys_sw_software.software_name LIKE 'Microsoft Visio Premium %') AND oa_group_sys.group_id = @group GROUP BY system.system_id]]></report_sql>
		<report_view_file>v_report</report_view_file>
		<report_view_contents></report_view_contents>
		<report_processing></report_processing>
		<report_sort_column>0</report_sort_column>
